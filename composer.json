    "homepage":    "https://github.com/urodoz/TimeExecutionStatsBundle",
    "license":     "MIT",

    "authors": [
        {
            "name": "Albert Lacarta",
            "email": "urodoz@gmail.com"
        }
    ],

    "require": {
        "php":                      ">=5.3.2",
        "symfony/framework-bundle": ">=2.1.0"
    },

    "minimum-stability": "dev",

    "autoload": {
        "psr-0": {
            "Urodoz\\TimeExecutionStatsBundle": ""
        }
    },
    
